Scanner scn = new Scanner(System.in);
int n = scn.nextInt();
int d = Scn.nextInt();
Int f = getDigitFrequency(n, d);
System.out.println(f);
}

public static int getdigitfrequency(int n, int d){

int rv = 0;

while(rv >0){
int dig = n % 10;
n = n /10;

if(dig == d){
rv++;
}
}
return rv;
}
}


input 
273725252
     2
     output 
     4
